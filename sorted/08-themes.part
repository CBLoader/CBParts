  <RulesElement name="Infuriating Taunt" type="Power" internal-id="ID_FMP_POWER_14168" source="Dragon Magazine 399" >
    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CLASS_0,ID_FMP_CATEGORY_36,ID_INTERNAL_CATEGORY_IMMEDIATE_REACTION,2 </Category>
