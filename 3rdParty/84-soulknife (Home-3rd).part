<RulesElement name="Psionic Assassin" type="Paragon Path" internal-id="ID_TIV_PARAGON_PATH_SOULKNIVES_3" source="Soulknives">
    <Prereqs> Soulknife and Shape Mind Weapon Power </Prereqs>
    <print-prereqs> Soulknife and Shape Mind Weapon Power </print-prereqs>
    <specific name="Class Features"> ID_TIV_CLASS_FEATURE_SOULKNIVES_3-1, ID_TIV_CLASS_FEATURE_SOULKNIVES_3-2, ID_TIV_CLASS_FEATURE_SOULKNIVES_3-3, ID_TIV_CLASS_FEATURE_SOULKNIVES_3-4, ID_TIV_CLASS_FEATURE_SOULKNIVES_3-5 </specific>
    <specific name="Powers"> ID_TIV_POWER_SOULKNIVES_3-1,ID_TIV_POWER_SOULKNIVES_3-3,ID_TIV_POWER_SOULKNIVES_3-2 </specific>
    <specific name="_DisplayPowers"> ID_TIV_POWER_SOULKNIVES_3-1,ID_TIV_POWER_SOULKNIVES_3-3,ID_TIV_POWER_SOULKNIVES_3-2 </specific>
