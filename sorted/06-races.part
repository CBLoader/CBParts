	<RulesElement name="Gift of Flight" type="Power" internal-id="ID_FMP_POWER_15839" source="Heroes of the Feywild">
		<Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_INTERNAL_CATEGORY_MINOR_ACTION,10,ID_FMP_CLASS_0 </Category>
