		<specific name="Short Description"> Combining potent divine prayers with martial prowess, you are a shield of faith. </specific>
		<specific name="_ParentClass"> ID_FMP_CLASS_2 </specific>
		<specific name="Trait Package"> Storm Domain, Sun Domain, Earth Domain, Death Domain </specific>
		<rules>
			<grant name="ID_INTERNAL_GRANTS_WARPRIEST" type="Grants" />
			<textstring name="_CLASSNAME" value="ID_FMP_CLASS_2 (ID_FMP_CLASS_705)" />
			<statadd name="_LEVEL-ONE-HPS" value="+12" />
			<statadd name="_PER-LEVEL-HPS" value="+5" />
