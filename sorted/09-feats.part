			<statadd name="weapon,crossbow group:attack" value="+1" type="Feat"/>
			<statadd name="weapon,crossbow group:attack" value="+2" type="Feat" requires="11 Level"/>
			<statadd name="weapon,crossbow group:attack" value="+3" type="Feat" requires="21 Level"/>
			<statadd name="wand implement,implement:attack" value="+1" type="Feat"/>
			<statadd name="wand implement,implement:attack" value="+2" type="Feat" requires="11 level"/>
			<statadd name="wand implement,implement:attack" value="+3" type="Feat" requires="21 level"/>
			<statadd name="implement,crossbow group:attack" value="+1" type="Feat"/>
