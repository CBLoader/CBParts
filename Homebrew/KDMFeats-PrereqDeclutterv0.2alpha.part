    <print-prereqs> Genasi, fighter </print-prereqs>
    <specific name="Tier"> Heroic </specific>
    <specific name="Short Description"> Bonuses to attack rolls and damage with earthshock </specific>
    <specific name="Special"/>
    <specific name="type"/>
    <specific name="Associated Power Info"/>
