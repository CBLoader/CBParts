      <specific name="Associated Power Info"/>
      <specific name="Associated Powers"/>
	  <specific name="_DisplayPowers"/>
	   <rules>
	     <modify name="Grab Attack" type="Power" Field=" Strangler" value="When you have combat advantage you gain another +2 to the attack roll. Also whenever you have a creature grabbed it suffers a -2 penalty to escape attempts and takes ongoing damage equal to your Strength modifier until it escapes."/>
