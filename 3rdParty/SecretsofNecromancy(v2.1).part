   <RulesElement name="Dwarven Weapon Proficiency (Sherigras/Dwarf)" type="Racial Trait" internal-id="ID_EOF_RACIAL_TRAIT_11006_DWARF_01" source="Player's Handbook" revision-date="7/1/2011">
