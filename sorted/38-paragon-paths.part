<RulesElement name="Traveler's Mummery" type="Power" internal-id="ID_FMP_POWER_10398" source="Dragon Magazine 382" revision-date="8/27/2010 11:54:10 AM">
      <Category> ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_ENCOUNTER,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_PARAGON_PATH_540,ID_FMP_CATEGORY_6,ID_FMP_CATEGORY_32,ID_FMP_CATEGORY_16,ID_INTERNAL_CATEGORY_STANDARD_ACTION,11 </Category>
      <Prereqs> Traveler's Harlequin </Prereqs>
