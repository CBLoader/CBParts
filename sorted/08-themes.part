<RulesElement name="Windlord" type="Theme" internal-id="ID_FMP_THEME_955" source="Heroes of the Elemental Chaos">
    <Flavor> "I soar, carried on currents of elemental air." </Flavor>
    <specific name="_DisplayPowers">ID_FMP_POWER_16114,ID_FMP_POWER_16115,ID_FMP_POWER_16116 </specific>
