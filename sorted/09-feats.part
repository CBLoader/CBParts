		<specific name="Tier"> Heroic </specific>
		<specific name="Short Description"> Gain resist 5/10/15 (by tier) cold, and +1/+2/+3 (by tier) to Will </specific>
		<specific name="Special"/>
		<specific name="Type"/>
		<specific name="Associated Power Info"/>
		<specific name="Associated Powers"/>
		<rules>
			<statadd name="Frozen Soul Cold" value="+5"/>
