	<RulesElement name="Hexblade" type="Class" internal-id="ID_FMP_CLASS_793" source="Player Essentials: Heroes of the Forgotten Kingdoms">
		<specific name="Key Abilities"> Charisma, Dexterity, Constitution, Intelligence </specific>
		<specific name="Implements"> Rods, wands </specific>
		<specific name="Armor Proficiencies"> Cloth, leather, hide, chainmail </specific>
