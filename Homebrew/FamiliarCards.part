  <RulesElement name="Familiar: Rootling" type="Power" internal-id="ID_TIV_FAMILIAR-21" source="Tivaan's Familiar Cards" revision-date="00/00/0000 00:00:00 AM">
    <Prereqs> Unselectable </Prereqs>
