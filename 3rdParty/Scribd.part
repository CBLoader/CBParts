      <grant name="ID_RYV_CLASS_FEATURE_42005-2" type="Class Feature" Level="11"/>
      <grant name="ID_RYV_CLASS_FEATURE_42005-3" type="Class Feature" Level="16"/>
      <grant name="ID_RYV_POWER_42063" type="Power" Level="11"/>
      <grant name="ID_RYV_POWER_42064" type="Power" Level="12"/>
      <grant name="ID_RYV_POWER_42065" type="Power" Level="20"/>
