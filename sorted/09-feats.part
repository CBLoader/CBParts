	<RulesElement name="Burn Everything" type="Feat" internal-id="ID_FMP_FEAT_3068" source="Dragon Magazine 388" revision-date="8/27/2010 12:20:56 PM">
		<Prereqs> Wizard </Prereqs>
		<print-prereqs> Wizard </print-prereqs>
		<specific name="Tier"> Heroic </specific>
