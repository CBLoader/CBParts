  <RulesElement name="Peerless Hunter" type="Paragon Path" internal-id="ID_FMP_PARAGON_PATH_789" source="Player Essentials: Heroes of the Forgotten Kingdoms">
    <Prereqs> Hunter </Prereqs>
    <print-prereqs> Hunter </print-prereqs>
    <specific name="Class Features"> ID_DBB_CLASS_FEATURE_100026, ID_DBB_CLASS_FEATURE_100027, ID_DBB_CLASS_FEATURE_100028, ID_DBB_CLASS_FEATURE_100029, ID_DBB_CLASS_FEATURE_1000030, ID_DBB_CLASS_FEATURE_100031</specific>
    <specific name="Powers"> ID_FMP_POWER_13613 </specific>
