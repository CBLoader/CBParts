  </RulesElement>
  <RulesElement name="Swarming Fey" type="Power" internal-id="ID_FMP_POWER_13606" source="Dragon Magazine 392" revision-date="12/06/2010 11:37:44 AM">
    <Category> ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_ENCOUNTER,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CLASS_134,ID_FMP_CATEGORY_33,ID_FMP_CATEGORY_24,ID_FMP_CATEGORY_39,ID_INTERNAL_CATEGORY_STANDARD_ACTION,13 </Category>
