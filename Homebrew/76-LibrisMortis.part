		 <grant name="ID_INTERNAL_BACKGROUND_ASSOCIATION_INSIGHT" type="Background Association" />
      </rules>
   </RulesElement>
   
   
   <RulesElement name="Wight" type="Race" internal-id="ID_DCY1_RACE_210" source="Libris Mortis, KDM Update" revision-date="01/23/2011" >
