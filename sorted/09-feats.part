	<RulesElement name="Haunting Shade" type="Feat" internal-id="ID_FMP_FEAT_3064" source="Dragon Magazine 387" revision-date="9/24/2010 9:28:35 AM">
		<Category> ID_INTERNAL_CATEGORY_MULTICLASS_SHADE,ID_INTERNAL_CATEGORY_MULTICLASS,ID_INTERNAL_CATEGORY_CLASS-SPECIFIC_MULTICLASS </Category>
