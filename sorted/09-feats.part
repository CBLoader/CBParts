	<RulesElement name="Mror Stalwart" type="Feat" internal-id="ID_FMP_FEAT_1402" source="Eberron Player's Guide" revision-date="8/27/2010 12:18:59 PM">
		<Prereqs> Dwarf, Mror Holds background </Prereqs>
		<print-prereqs> Dwarf, Mror Holds background </print-prereqs>
		<specific name="Tier"> Heroic </specific>
