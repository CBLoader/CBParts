  <RulesElement name="Dauntless Gaze" type="Power" internal-id="ID_FMP_POWER_16002" source="Dragon Magazine 404">
    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CATEGORY_7,ID_FMP_CATEGORY_36,ID_INTERNAL_CATEGORY_IMMEDIATE_INTERRUPT,6,ID_FMP_CLASS_0 </Category>
