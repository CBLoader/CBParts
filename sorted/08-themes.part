    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_INTERNAL_CATEGORY_IMMEDIATE_INTERRUPT,ID_FMP_CLASS_0,10,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_38 </Category>
