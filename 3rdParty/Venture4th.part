    <specific name="Ghost Pact"> If the target has vulnerability to cold, you gain temporary hit points equal to your Intelligence modifier. </specific>
    <specific name="_Associated Feats"/>
    <specific name="Class"> ID_FMP_CLASS_7 </specific>
    <specific name="Level"> 1 </specific>
    <specific name="Power Type"> Attack </specific>
  </RulesElement>
  <RulesElement name="Spirit Whispers" type="Power" internal-id="ID_CDJ_POWER_645004" source="Venture 4th" revision-date="8/27/2010 11:25:07 AM">
    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CLASS_7,ID_FMP_CATEGORY_31,ID_INTERNAL_CATEGORY_MINOR_ACTION,2 </Category>
