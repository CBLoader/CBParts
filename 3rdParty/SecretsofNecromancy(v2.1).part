   <RulesElement name="Eladrin Will (Eladrin Body)" type="Racial Trait" internal-id="ID_EOF_RACIAL_TRAIT_11006_BODY_TYPE_ELADRIN_04" source="Player's Handbook" revision-date="7/1/2011">
