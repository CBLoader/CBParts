           <statadd name="AC" value="+ABILITYMOD(wis)" type="Class Feature" condition="against opportunity attacks" />
		   <grant name="ID_INTERNAL_SKILL_TRAINING_PERCEPTION" type="Skill Training" />
		   <statadd name="Perception Misc" value="+3" type="Class Feature" requires="Perception Skill Training"/>
        </rules>
