		<Category> ID_INTERNAL_CATEGORY_CLASS_POWER,ID_INTERNAL_CATEGORY_UTILITY,ID_FMP_CLASS_FEATURE_3673,ID_INTERNAL_CATEGORY_SWAPPABLE_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_CLASS_7,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_9,ID_FMP_CATEGORY_38,ID_INTERNAL_CATEGORY_MINOR_ACTION,16 </Category>
