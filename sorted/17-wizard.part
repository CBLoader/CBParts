<RulesElement name="Bladesong" type="Power" internal-id="ID_FMP_POWER_14285" source="Player Essentials: Neverwinter Campaign Guide">
   <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CLASS_9,ID_FMP_CATEGORY_31,ID_INTERNAL_CATEGORY_MINOR_ACTION </Category>
