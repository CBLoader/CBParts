<!-- Class Acts: Seekers of the Moonrise Vanguard (Dragon Magazine 413) -->
<RulesElement name="Moonrise Stalker" type="Paragon Path" internal-id="ID_TIV_PARAGON_PATH_DRAGONMAGAZINE413-1" source="Dragon Magazine 413">
