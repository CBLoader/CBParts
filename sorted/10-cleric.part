	<RulesElement name="Smite Undead" type="Power" internal-id="ID_FMP_POWER_12601" source="Player Essentials: Heroes of the Fallen Lands" revision-date="10/12/2010 1:29:33 PM">
		<Category> ID_FMP_CLASS_FEATURE_2740,ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_ENCOUNTER,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CLASS_2,ID_FMP_CATEGORY_67,ID_FMP_CATEGORY_32,ID_FMP_CATEGORY_2,ID_FMP_CATEGORY_24,ID_INTERNAL_CATEGORY_STANDARD_ACTION,ID_INTERNAL_CATEGORY_MISS </Category>
