  <RulesElement name="Battleweaver" type="Paragon Path" internal-id="ID_FMP_PARAGON_PATH_830" source="Player Essentials: Heroes of Shadow" revision-date="4/8/2011 10:06:21 AM">
    <Prereqs> Any arcane class </Prereqs>
    <print-prereqs> Any arcane class, at least two illusion at-will or encounter powers </print-prereqs>
    <specific name="Power Name" />
    <specific name="Trait Package" />
    <specific name="Class Features"> ID_FMP_CLASS_FEATURE_3766, ID_FMP_CLASS_FEATURE_3767, ID_FMP_CLASS_FEATURE_3768, ID_FMP_CLASS_FEATURE_3829, ID_FMP_CLASS_FEATURE_3830, ID_FMP_CLASS_FEATURE_3831 </specific>
    <specific name="Powers"> ID_FMP_POWER_14034,ID_FMP_POWER_14035,ID_FMP_POWER_14036 </specific>
