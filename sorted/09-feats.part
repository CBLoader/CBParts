	<RulesElement name="Dusk Elf Stealth" type="Feat" internal-id="ID_FMP_FEAT_2156" source="Dragon Magazine 382" revision-date="8/27/2010 12:19:50 PM">
		<Category> ID_INTERNAL_INTERNAL_ELF_BLOODLINE </Category>
