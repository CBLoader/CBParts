		<specific name="Rarity"> Common </specific>
	</RulesElement>

	<RulesElement name="Greater Flying Carpet (paragon tier)" type="Magic Item" internal-id="ID_TIV_MAGIC_ITEM_MME_WONDROUS_17-1" source="Mordenkainen's Magnificent Emporium">
