  <RulesElement name="Lightning Robe +1" type="Magic Item" internal-id="ID_FMP_MAGIC_ITEM_TIV_10025_14_01" source="Buck-A-Batch: Magic Armor (Book 1)" revision-date="12/8/2010 10:29:51 AM">
