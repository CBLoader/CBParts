<RulesElement name="Bugbear Strangler" type="Feat" internal-id="ID_DCY1_FEAT_BUGB001" source="DC Homebrewed" revision-date="07/05/2011">
      <Prereqs> Str 14, ID_FMP_RACE_10 </Prereqs>
      <print-prereqs> Bugbear, Str 14 </print-prereqs>
