</RulesElement>
  <RulesElement name="Defensive Lore" type="Power" internal-id="ID_FMP_POWER_14140" source="Dragon Magazine 399" >
    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_CLASS_0,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_2,ID_FMP_CATEGORY_39,ID_INTERNAL_CATEGORY_MINOR_ACTION,10,ID_FMP_CLASS_ORDER_ADEPT </Category>
