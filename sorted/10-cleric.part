	<RulesElement name="Conquering Blow" type="Power" internal-id="ID_TIV_POWER_DRAGON_409_DOMINATION-6" source="Dragon Magazine 409">
		<Category> ID_TIV_CLASS_FEATURE_DRAGON_409-3,ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_ENCOUNTER,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CLASS_2,ID_FMP_CATEGORY_32,ID_FMP_CATEGORY_24,ID_FMP_CATEGORY_38,ID_FMP_CATEGORY_7,ID_INTERNAL_CATEGORY_STANDARD_ACTION,3 </Category>
