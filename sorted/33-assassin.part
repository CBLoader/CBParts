  <RulesElement name="Wyven Venom Recipe" type="Class Feature" internal-id="ID_HOS_CLASS_FEATURE_WYVERN_VENOM_RECIPE" source="Player Essentials: Heroes of Shadow" revision-date="10/14/2010 9:29:04 AM">
