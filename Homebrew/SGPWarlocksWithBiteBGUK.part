    <specific name="Target"> One creature </specific>
    <specific name="Attack"> Charisma or Constitution vs. AC </specific>
    <specific name="Hit"> 1d8 + Charisma or Constitution modifier force damage. Make a secondary attack. </specific>
    <specific name=" Secondary Attack"> Charisma or Constitution vs. Fortitude </specific>
    <specific name=" Hit"> 2d8 + Charisma or Constitution modifier damage. </specific>
    <specific name="Effect"> The target is pushed 1 square and takes a -1 to all attack rolls until the start of your next turn. </specific>
    <specific name="_Associated Feats"/>
    <specific name="Class"> ID_FMP_CLASS_7 </specific>
    <specific name="Level"> 7 </specific>
    <specific name="Power Type"> Attack </specific>
  </RulesElement>
