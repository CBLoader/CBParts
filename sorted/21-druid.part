      <grant name="ID_WOG_SENTINEL_CLASS_FEATURE_2B" type="Class Feature" requires="ID_WOG_SENTINEL_TRAIT_PACKAGE_2|ID_CDJ_HYBRID_SENTINEL_TRAIT_PACKAGE_2"/>
