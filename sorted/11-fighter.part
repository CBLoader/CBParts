<RulesElement name="Brawler Style" type="Class Feature" internal-id="ID_FMP_CLASS_FEATURE_2182" source="Martial Power 2" revision-date="10/5/2010 1:47:06 PM">
      <Category> ID_FMP_CLASS_FEATURE_1064,1 </Category>
      <specific name="Short Description" />
