		<specific name="type"/>
		<specific name="Associated Power Info"/>
		<specific name="Associated Powers"/>
		<rules>
			<statadd name="attack rolls" value="+1" condition="while bloodied"/>
			<statadd name="damage rolls" value="+1" condition="while bloodied"/>
