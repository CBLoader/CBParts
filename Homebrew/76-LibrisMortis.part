<RulesElement name="Necrotic Eruption" type="Power" internal-id="ID_DCY1_POWER_CYST03" source="Libris Mortis, KDM Update" revision-date="01/27/2011" >
  <Category> ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_INTERNAL_CATEGORY_MISS,ID_FMP_CATEGORY_12,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_25,ID_INTERNAL_CATEGORY_STANDARD_ACTION </Category>
