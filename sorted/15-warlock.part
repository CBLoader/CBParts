	<RulesElement name="Murky Deep" type="Power" internal-id="ID_FMP_POWER_16266" source="Heroes of the Elemental Chaos">
		<Category> ID_INTERNAL_CATEGORY_CLASS_POWER,ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_CLASS_7,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_74,ID_FMP_CATEGORY_39,ID_INTERNAL_CATEGORY_MINOR_ACTION,10 </Category>
