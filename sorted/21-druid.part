  <RulesElement name="Wild Shape" type="Power" internal-id="ID_FMP_POWER_5032" source="Player's Handbook 2,Player's Handbook 3" revision-date="11/05/2010 00:00:00 AM">
    <Prereqs> Unselectable </Prereqs>
