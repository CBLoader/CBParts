    <grant name="ID_INTERNAL_POWER_RANGED_BASIC_ATTACK" type="Power" />
    <grant name="ID_INTERNAL_POWER_BULL_RUSH_ATTACK" type="Power" />
    <grant name="ID_INTERNAL_POWER_GRAB_ATTACK" type="Power" />
    <grant name="ID_INTERNAL_POWER_OPPORTUNITY_ATTACK" type="Power" />
	<grant name="ID_KDM_POWER_SECOND_WIND" type="Power" />
