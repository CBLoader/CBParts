    <rules>
    <grant name="ID_FMP_CLASS_FEATURE_3037" type="Class Feature"/>
   </rules>
</AppendNodes>

 <RulesElement name="Conjuration Apprentice" type="Class Feature" internal-id="ID_CDJ_CLASS_FEATURE_2878" source="Player Essentials: Heroes of the Fallen Lands" revision-date="10/14/2010 9:29:04 AM">
