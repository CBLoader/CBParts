   <RulesElement name="Vampire-Spawn - Stolen Child" type="Background" internal-id="ID_DCY1_BACKGROUND_VS01" source="Libris Mortis, KDM Update" revision-date="01/27/2011" >
      <Prereqs> ID_DCY1_RACE_209 </Prereqs>
