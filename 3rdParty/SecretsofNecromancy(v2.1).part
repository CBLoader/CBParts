   <RulesElement name="Undead (Sherigras/Revenant)" type="Racial Trait" internal-id="ID_EOF_RACIAL_TRAIT_11006_REVENANT_02" source="Dragon Magazine 376" revision-date="7/1/2011">
      <Category> ID_EOF_11006_HYBRID_RACE_(REVENANT) </Category>
