<RulesElement name="Blood Drinker's Pounce" type="Power" internal-id="ID_FMP_POWER_13866" source="Player Essentials: Heroes of Shadow" revision-date="3/10/2011 12:51:49 PM">
  <Category> ID_INTERNAL_CATEGORY_CLASS_POWER,ID_INTERNAL_CATEGORY_UTILITY,ID_FMP_CLASS_FEATURE_3639,ID_INTERNAL_CATEGORY_SWAPPABLE_UTILITY,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CLASS_817,ID_FMP_CATEGORY_38,ID_INTERNAL_CATEGORY_FREE_ACTION,22 </Category>
