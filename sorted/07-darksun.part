  <RulesElement name="Battle Slave Action" type="Class Feature" internal-id="ID_FMP_CLASS_FEATURE_2495" source="Dark Sun Campaign Setting" revision-date="10/14/2010 9:29:04 AM">
    <Category> 11 </Category>
    <specific name="Short Description"> Action Point (action): +4 to melee basic attack rolls after action point use </specific>
