  <RulesElement name="Hourglass of the Warrior-Oracle" type="Magic Item" internal-id="ID_FOURTHCORE_MAGIC_ITEM_HOURGLASS_OF_THE_WARRIOR_ORACLE" source="Fourthcore Armory">
