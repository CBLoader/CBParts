  <RulesElement name="Shadowrift Armor +1" type="Magic Item" internal-id="ID_FMP_MAGIC_ITEM_TIV_10025_46_01" source="Buck-A-Batch: Magic Armor (Book 2)" revision-date="12/8/2010 10:29:51 AM">
    <Flavor> This black armor is carved from living shadow through a series of controversial rituals. </Flavor>
    <specific name="Level"> 5 </specific>
    <specific name="Gold"> 1000 </specific>
    <specific name="Magic Item Type"> Armor </specific>
    <specific name="Tier"/>
    <specific name="_Item_Set_ID"/>
    <specific name="Armor"> Cloth, Leather, Hide, Chain </specific>
    <specific name="Weapon"/>
    <specific name="Item Slot"/>
    <specific name="Requirement"/>
    <specific name="Critical"/>
    <specific name="Special"/>
