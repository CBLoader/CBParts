      <statadd name="ranged basic,arena group:attack" value="+War Wizard's Expertise" type="Feat" requires="ID_INTERNAL_ARENA_WEAPON_CATEGORY_HEAVY_BLADE|ID_INTERNAL_ARENA_WEAPON_CATEGORY_LIGHT_BLADE"/>
      <statadd name="arcane,arena group:attack" value="+War Wizard's Expertise" type="Feat" requires="ID_INTERNAL_ARENA_WEAPON_CATEGORY_HEAVY_BLADE|ID_INTERNAL_ARENA_WEAPON_CATEGORY_LIGHT_BLADE"/>
      <statadd name="ranged basic,heavy blade group:attack" value="+War Wizard's Expertise" type="Feat"/>
      <statadd name="ranged basic,light blade group:attack" value="+War Wizard's Expertise" type="Feat"/>
