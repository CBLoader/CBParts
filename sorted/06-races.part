	<RulesElement name="Deceitful Image" type="Power" internal-id="ID_FMP_POWER_13732" source="Dragon Magazine 393" revision-date="8/27/2010 11:39:43 AM">
		<Category> ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_ENCOUNTER,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_PARAGON_PATH_800,ID_FMP_CATEGORY_10,ID_INTERNAL_CATEGORY_STANDARD_ACTION </Category>
