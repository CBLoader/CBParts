<RulesElement name="Aboleth Slime Concentrate Recipe" type="Class Feature" internal-id="ID_HOS_CLASS_FEATURE_ABOLETH_RECIPE" source="Dragon Magazine 423" revision-date="5/20/2013 12:00:00 AM">
