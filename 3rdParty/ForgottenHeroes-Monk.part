    <specific name="Powers"> ID_CDJ_POWER_2150148,ID_CDJ_POWER_2150149,ID_CDJ_POWER_2150150 </specific>
    <specific name="_DisplayPowers"> ID_CDJ_POWER_2150148,ID_CDJ_POWER_2150149,ID_CDJ_POWER_2150150 </specific>
