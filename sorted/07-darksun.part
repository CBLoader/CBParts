  <RulesElement name="Arena Training" type="Class Feature" internal-id="ID_FMP_CLASS_FEATURE_2893" source="Dark Sun Campaign Setting" revision-date="10/14/2010 9:29:04 AM">
    <Category> ID_FMP_CLASS_FEATURE_1064,1 </Category>
    <specific name="Short Description"> Grants proficiency with up to 2 additional weapons. Increases AC bonus, improvised weapon bonus and damage </specific>
    <specific name="Level"> 1 </specific>
    <rules>
      <statadd name="Arena Training" value="+1" />
      <statadd name="Arena Training" value="+1" requires="Paragon Tier" />
      <statadd name="Arena Training" value="+1" requires="Epic Tier" />
