<RulesElement name="Avascular Mass (Wizard)" type="Power" internal-id="ID_DCY1_POWER_LMWIZ02" source="Libris Mortis, KDM Update" revision-date="02/08/2011" >
  <Category> ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_DAILY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_INTERNAL_CATEGORY_MISS,ID_FMP_CLASS_9,ID_FMP_CATEGORY_26,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_25,ID_FMP_CATEGORY_12,ID_INTERNAL_CATEGORY_STANDARD_ACTION,9 </Category>
