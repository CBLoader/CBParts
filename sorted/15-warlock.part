		<rules>
			<grant name="ID_FMP_POWER_16258" type="Power" />
			<grant name="ID_FMP_POWER_16259" type="Power" />
			<grant name="ID_INTERNAL_PROFICIENCY_WEAPON_PROFICIENCY_(BLADE_OF_CHAOS)" type="proficiency" />
			<statadd name="FREEBEE:ID_TIV_HEXBLADE_TRAIT_WEAPON_BLADE_OF_CHAOS" value="+1" />
			<statadd name="ID_FMP_WEAPON_171:attack" value="+Offhand Implement Bonus" type="off-hand enhancement" requires="WearingPactBlade&amp;WearingRod" />
			<statadd name="ID_FMP_WEAPON_171:damage" value="+Offhand Implement Bonus" type="off-hand enhancement" requires="WearingPactBlade&amp;WearingRod" />
			<statadd name="ID_FMP_WEAPON_171:attack" value="+Offhand Implement Bonus" type="off-hand enhancement" requires="WearingPactBlade&amp;WearingWand" />
