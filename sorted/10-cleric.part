	</RulesElement>
	<RulesElement name="Stone Speak" type="Power" internal-id="ID_FMP_POWER_13708" source="Dragon Magazine 392" revision-date="12/01/2010 1:29:48 PM">
		<Category> ID_FMP_CLASS_FEATURE_3438,ID_INTERNAL_CATEGORY_ENCOUNTER,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CLASS_2,ID_FMP_CATEGORY_24,ID_FMP_CATEGORY_32,ID_INTERNAL_CATEGORY_MINOR_ACTION </Category>
