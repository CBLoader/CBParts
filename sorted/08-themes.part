<RulesElement name="Cormyrian Battle Mage" type="Theme" internal-id="ID_FMP_THEME_974" source="Dragon Magazine 407">
  <prereqs> Wizard or sorcerer </prereqs>
  <specific name="_DisplayPowers"> ID_FMP_POWER_16371,ID_FMP_POWER_16372,ID_FMP_POWER_16373 </specific>
  <specific name="_PARSED_SUB_FEATURES"> ID_FMP_CLASS_FEATURE_4423,ID_FMP_CLASS_FEATURE_4424,ID_FMP_CLASS_FEATURE_4425 </specific>
