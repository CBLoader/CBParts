<RulesElement name="Primordial Adept" type="Theme" internal-id="ID_FMP_THEME_950" source="Heroes of the Elemental Chaos">
    <specific name="_DisplayPowers">ID_FMP_POWER_16095,ID_FMP_POWER_16096,ID_FMP_POWER_16097,ID_FMP_POWER_16098,ID_FMP_POWER_16099 </specific>
