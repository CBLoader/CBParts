<RulesElement name="Weapon Unity" type="Power" internal-id="ID_FMP_POWER_16084" source="Heroes of the Elemental Chaos">
    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_CATEGORY_74,ID_FMP_CATEGORY_28,ID_INTERNAL_CATEGORY_MINOR_ACTION,ID_FMP_CLASS_0,10 </Category>
