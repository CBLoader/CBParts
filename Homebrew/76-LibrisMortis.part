      <specific name="Average Weight"> 40-70 lb. </specific>
      <specific name="Ability Scores"> +2 Strength, +2 Dexterity or +2 Constitution </specific>
      <specific name="Languages"> Common, one language you may have spoken in life </specific>
      <specific name="Skill Bonuses"> +2 Intimidate, +2 Endurance </specific>
      <specific name="Male Names">  </specific>
      <specific name="Female Names">  </specific>
      <specific name="Short Description">  </specific>
      <specific name="Racial Traits"> ID_DCY1_RACIAL_TRAIT_7505, ID_DCY1_RACIAL_TRAIT_7515, ID_FMP_RACIAL_TRAIT_1866, ID_FMP_RACIAL_TRAIT_1814, ID_FMP_RACIAL_TRAIT_1867, ID_FMP_RACIAL_TRAIT_1868 </specific>
