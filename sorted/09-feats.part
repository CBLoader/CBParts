	</RulesElement>
	<RulesElement name="Miryath's First Strike" type="Feat" internal-id="ID_FMP_FEAT_1677" source="Player's Handbook 3" revision-date="8/27/2010 12:19:20 PM">
		<Prereqs> Wis 13, githzerai </Prereqs>
		<print-prereqs> Wis 13, githzerai </print-prereqs>
		<specific name="Tier"> Heroic </specific>
		<specific name="Short Description"> 1/2/3 (by tier) + Wis or Dex modifier extra damage the first time you hit creature that has not yet acted </specific>
		<specific name="Special"/>
		<specific name="type"/>
		<specific name="Associated Power Info"/>
		<specific name="Associated Powers"/>
