	<RulesElement name="Alchemist's Essence" type="Ritual" internal-id="ID_UNDR_MME_ALCHEMISTS_ESSENCE_RITUAL" source="Mordenkainen's Magnificent Emporium">
		<specific name="Category"> Volatile </specific>
		<specific name="Key Skill"> Arcana or Thievery (no check) </specific>
