	</RulesElement>
	<RulesElement name="Trap-Gang Method" type="Power" internal-id="ID_FMP_POWER_16468" source="Dungeon Survival Handbook">
		<Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_AT-WILL_USAGE,ID_FMP_CLASS_0,ID_INTERNAL_CATEGORY_IMMEDIATE_INTERRUPT,10 </Category>
