</RulesElement>
<RulesElement name="Sidhe Bargain" type="Power" internal-id="ID_FMP_POWER_15886" source="Heroes of the Feywild" >
    <Category> ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_INTERNAL_CATEGORY_UTILITY,ID_FMP_CLASS_0,ID_FMP_CATEGORY_31,ID_INTERNAL_CATEGORY_STANDARD_ACTION,2 </Category>
    <Prereqs> Sidhe Lord </Prereqs>
