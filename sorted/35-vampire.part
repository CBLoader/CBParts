  <specific name="Class"> ID_FMP_CLASS_817 </specific>
  <specific name="Level"> 22 </specific>
  <specific name="Power Type"> Utility </specific>
  <specific name="_ParentFeature"> ID_FMP_CLASS_FEATURE_3639 </specific>
  <specific name="_Subclasses"> ID_FMP_CLASS_817 </specific>
