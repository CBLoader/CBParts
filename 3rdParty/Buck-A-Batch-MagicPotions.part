  <RulesElement name="Darkside Potion (level 6)" type="Magic Item" internal-id="ID_TIV_MAGIC_ITEM_10040_03" source="Buck-A-Batch Magic Potions (Book 1)" revision-date="12/14/2010 10:35 AM">
