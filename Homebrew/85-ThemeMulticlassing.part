  <RulesElement name="Precise Stalker" type="Feat" internal-id="ID_RYV_FEAT_110009" source="At-Will Blog" revision-date="8/27/2010 12:17:55 PM">
    <Category> ID_INTERNAL_CATEGORY_MULTICLASS_RANGER,ID_INTERNAL_CATEGORY_MULTICLASS,ID_INTERNAL_CATEGORY_CLASS-SPECIFIC_MULTICLASS </Category>
    <Prereqs> Dex 13 or Str 13; ~MULTICLASS or Unlimited Multiclass or Ranger CountsAsClass; !ID_FMP_CLASS_5; !ID_FMP_HYBRID_CLASS_355 </Prereqs>
    <print-prereqs> Dex 13 or Str 13 </print-prereqs>
