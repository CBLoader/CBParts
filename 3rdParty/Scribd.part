  <RulesElement name="Aerial Acrobat" type="Paragon Path" internal-id="ID_RYV_PARAGON_PATH_42005" source="Avatar" revision-date="8/27/2010 12:40:19 PM">
    <Prereqs> Airbender </Prereqs>
    <print-prereqs> Airbender </print-prereqs>
    <Flavor> "I am a leaf on the wind, watch me soar." </Flavor>
    <specific name="Power Name"/>
    <specific name="Class Features"> ID_RYV_CLASS_FEATURE_42005-1, ID_RYV_CLASS_FEATURE_42005-2, ID_RYV_CLASS_FEATURE_42005-3 </specific>
    <specific name="Powers"> ID_RYV_POWER_42063,ID_RYV_POWER_42064,ID_RYV_POWER_42065 </specific>
