    <specific name="Gold"> 13000 </specific>
    <specific name="Magic Item Type"> Feet Slot Item </specific>
    <specific name="Tier"/>
    <specific name="_Item_Set_ID"/>
    <specific name="Armor"/>
    <specific name="Weapon"/>
    <specific name="Item Slot"> Feet </specific>
    <specific name="Requirement"/>
    <specific name="Critical"/>
    <specific name="Special"/>
    <specific name="Power"> Power (Daily): Immediate Reaction. Use this power when the space you are standing in  becomes targeted by a spell with a range of blast, burst, or zone. You shift to an open space outside of the attack's range, provided that there's one available within 5 squares of you. </specific>
    <specific name="_Rarity"> Common </specific>
    <specific name="Granted Powers"/>
    <specific name="Property"> You gain a +2 item bonus to Reflex defense when defending against spells. </specific>
    <rules>
      <statadd name="Reflex Defense" value="+2" type="Item" condition="when defending against spells"/>
    </rules>
  </RulesElement>
  <RulesElement name="Fire Trail Boots (herioc tier)" type="Magic Item" internal-id="ID_FMP_MAGIC_ITEM_TIV_10030_09" source="Buck-A-Batch Magic Boots (Book 1)" revision-date="12/10/2010 10:34:01 AM">
