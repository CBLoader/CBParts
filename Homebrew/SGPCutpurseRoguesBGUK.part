  <RulesElement name="Cutpurse  " type="Feat" internal-id="ID_FMP_FEAT_65121" source="St Georges Prey" revision-date="8/27/2010 12:20:06 PM">
    <Prereqs> Rogue </Prereqs>
    <print-prereqs> Rogue, wielding a dagger </print-prereqs>
    <specific name="Tier"> Heroic </specific>
    <specific name="Short Description"> Apply weapon bonus to Thievery checks </specific>
    <specific name="Special"/>
    <specific name="type"/>
    <specific name="Associated Power Info"/>
