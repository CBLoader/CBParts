	<RulesElement name="Angelic Avenger" type="Paragon Path" internal-id="ID_FMP_PARAGON_PATH_17" source="Player's Handbook" revision-date="8/27/2010 12:40:15 PM">
		<Prereqs> Cleric class </Prereqs>
		<print-prereqs> Cleric class </print-prereqs>
		<Flavor> "I am as an angel, an avenger for my god." </Flavor>
		<specific name="Power Name"> Prayers </specific>
		<specific name="Class Features"> ID_FMP_CLASS_FEATURE_441, ID_FMP_CLASS_FEATURE_445, ID_FMP_CLASS_FEATURE_672, ID_FMP_CLASS_FEATURE_440 </specific>
