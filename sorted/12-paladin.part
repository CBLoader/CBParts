			<grant name="ID_WOG_CAVALIER_CLASS_FEATURE_2B" type="Class Feature" requires="ID_WOG_CAVALIER_TRAIT_PACKAGE_2|ID_CDJ_HYBRID_CAVALIER_VALOR"/>
