   <RulesElement name="Dual Soul (Sherigras/Kalashtar)" type="Racial Trait" internal-id="ID_EOF_RACIAL_TRAIT_11006_KALASHTAR_01" source="Eberron Player's Guide" revision-date="7/1/2011">
