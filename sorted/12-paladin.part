			<grant name="ID_WOG_VALIANT_CAVALIER_CLASS_FEATURE_4A" type="Class Feature" requires="ID_WOG_CAVALIER_TRAIT_PACKAGE_1|ID_CDJ_HYBRID_CAVALIER_SACRIFICE"/>
