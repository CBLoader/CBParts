    <specific name="Class"> ID_FMP_THEME_856 </specific>
    <specific name="Level"> 1 </specific>
  </RulesElement>
  <RulesElement name="Battle Riding" type="Power" internal-id="ID_FMP_POWER_14146" source="Dragon Magazine 399" >
    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CLASS_0,ID_FMP_CATEGORY_36,ID_INTERNAL_CATEGORY_IMMEDIATE_INTERRUPT,2 </Category>
