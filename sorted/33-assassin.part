  <RulesElement name="Grell Bile Recipe" type="Class Feature" internal-id="ID_HOS_CLASS_FEATURE_GRELL_RECIPE" source="Dragon Magazine 423" revision-date="5/20/2013 12:00:00 AM">
    <Category> Assassin Poison,15 </Category>
