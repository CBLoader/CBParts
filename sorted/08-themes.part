  <RulesElement name="Honorable Resolve" type="Power" internal-id="ID_FMP_POWER_16001" source="Dragon Magazine 404">
    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_CATEGORY_36,ID_INTERNAL_CATEGORY_IMMEDIATE_REACTION,2,ID_FMP_CLASS_0 </Category>
