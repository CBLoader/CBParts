</RulesElement>
  <RulesElement name="Hellfire Step" type="Power" internal-id="ID_FMP_POWER_15946" source="The Book of Vile Darkness">
    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CATEGORY_32,ID_FMP_CATEGORY_19,ID_FMP_CATEGORY_39,ID_INTERNAL_CATEGORY_MOVE_ACTION,6,ID_FMP_CLASS_0 </Category>
