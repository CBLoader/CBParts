    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CATEGORY_68,ID_FMP_CATEGORY_74,ID_INTERNAL_CATEGORY_MINOR_ACTION,ID_FMP_CLASS_0,2 </Category>
