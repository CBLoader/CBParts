  <RulesElement name="Unholy Aura" type="Power" internal-id="ID_FMP_POWER_15969" source="The Book of Vile Darkness">
    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_PARAGON_PATH_931,ID_FMP_CATEGORY_68,ID_FMP_CATEGORY_32,ID_FMP_CATEGORY_7,ID_INTERNAL_CATEGORY_MINOR_ACTION,12 </Category>
