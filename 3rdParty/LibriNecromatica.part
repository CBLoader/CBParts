      <grant name="ID_TIV_CLASS_FEATURE_LIBRA_NECROMATICA_40_02" type="Class Feature" Level="11" />
      <grant name="ID_TIV_CLASS_FEATURE_LIBRA_NECROMATICA_40_03" type="Class Feature" Level="16" />
