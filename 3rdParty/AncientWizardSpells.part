<RulesElement name="Spectral Hunter of Yore" type="Power" internal-id="ID_FMP_POWER_TIV_10000_07" source="Ancient Wizard Magic" revision-date="11/21/2010 11:38:30 AM">
    <Category> ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_DAILY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_CLASS_9,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_26,ID_FMP_CATEGORY_8,ID_INTERNAL_CATEGORY_MINOR_ACTION,1 </Category>
