    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_DAILY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_CLASS_104,ID_FMP_CATEGORY_10,ID_FMP_CATEGORY_31,ID_INTERNAL_CATEGORY_IMMEDIATE_REACTION,6 </Category>
