  <RulesElement name="Traveler's Unpredictable Power (Holy Barrage)" type="Class Feature" internal-id="ID_INTERNAL_CLASS_FEATURE_TRAVELER'S_UNPREDICTABLE_POWER_(HOLY_BARRAGE)" source="Dragon Magazine 382">
