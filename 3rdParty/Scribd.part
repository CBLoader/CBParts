  <RulesElement name="Moon's Blessing" type="Class Feature" internal-id="ID_RYV_CLASS_FEATURE_42004-2" source="Avatar" revision-date="10/5/2010 1:47:04 PM">
    <Category> ID_RYV_HYBRID_CLASS_42004-1,1 </Category>
