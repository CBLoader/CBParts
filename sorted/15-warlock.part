			<grant name="ID_WOG_COMPANION_7_POWERCARD_2" type="Power" />
			<grant name="ID_WOG_COMPANION_7_POWERCARD_3" type="Power" />
			<grant name="ID_WOG_COMPANION_7_POWERCARD_4" type="Power" />
