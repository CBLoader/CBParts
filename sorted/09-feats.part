			<statadd name="staff implement,implement:attack" value="+1" type="Feat"/>
			<statadd name="staff implement,implement:attack" value="+2" type="Feat" requires="11 level"/>
			<statadd name="staff implement,implement:attack" value="+3" type="Feat" requires="21 level"/>
			<statadd name="weapon,staff group:attack" value="+1" type="Feat"/>
			<statadd name="weapon,staff group:attack" value="+2" type="Feat" requires="11 level"/>
			<statadd name="weapon,staff group:attack" value="+3" type="Feat" requires="21 level"/>
