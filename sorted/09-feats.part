			<statadd name="arcane,fire:damage" value="+Draconic Spellcaster" type="feat" requires="Dragon Breath Fire"/>
			<statadd name="arcane,lightning:damage" value="+Draconic Spellcaster" type="feat" requires="Dragon Breath Lightning"/>
