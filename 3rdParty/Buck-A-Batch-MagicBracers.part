  <RulesElement name="Bloody Bracers (herioc tier)" type="Magic Item" internal-id="ID_TIV_MAGIC_ITEM_10035_02_01" source="Buck-A-Batch Magic Bracers (Book 1)" revision-date="12/11/2010 10:34:01 AM">
