      <specific name="_SkillPower" />
      <specific name="Display"> Psion Attack 5 </specific>
      <specific name="Keywords"> Implement, Psionic, Psychic, Zone </specific>
      <specific name="Action Type"> Standard action </specific>
      <specific name="Attack Type"> Area burst 1 within 10 squares </specific>
      <specific name="Target"> Each enemy in burst </specific>
