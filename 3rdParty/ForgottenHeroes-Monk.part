  <RulesElement name="Monastic Tradition (Hybrid)" type="Class Feature" internal-id="ID_FMP_CLASS_FEATURE_2413" source="Player's Handbook 3" revision-date="10/5/2010 1:47:06 PM">
    <Category> 1 </Category>
    <specific name="Short Description"> Gain monk striker damage from a monastic tradition </specific>
    <specific name="Level"> 1 </specific>
    <specific name="_PARSED_SUB_FEATURES"> ID_FMP_CLASS_FEATURE_2532, ID_FMP_CLASS_FEATURE_2533, ID_CDJ_CLASS_FEATURE_2150008, ID_CDJ_CLASS_FEATURE_2150009, ID_CDJ_CLASS_FEATURE_2150010, ID_CDJ_CLASS_FEATURE_2150011 </specific>
