    <specific name="Gold"> 840 </specific>
    <specific name="Magic Item Type"> Arms Slot Item </specific>
    <specific name="Tier"/>
    <specific name="_Item_Set_ID"/>
    <specific name="Armor"/>
    <specific name="Weapon"/>
    <specific name="Item Slot"> Arms </specific>
    <specific name="Requirement"/>
    <specific name="Critical"/>
    <specific name="Special"/>
    <specific name="Power"> Power (Daily): Immediate Interrupt. Use this ability when targeted by an attack that affects your will. You gain a +3 bonus to your Will vs. the attack. </specific>
    <specific name="_Rarity"> Common </specific>
    <specific name="Granted Powers"/>
    <specific name="Property"> You gain a +1 item bonus to your Insight and Perception skill checks. </specific>
    <rules>
      <statadd name="Insight Misc" value="+1" type="item"/>
      <statadd name="Perception Misc" value="+1" type="item"/>
    </rules>
  </RulesElement>
  <RulesElement name="Bracers of Perseverance (herioc tier)" type="Magic Item" internal-id="ID_TIV_MAGIC_ITEM_10035_05" source="Buck-A-Batch Magic Bracers (Book 1)" revision-date="12/11/2010 10:34:01 AM">
