  <RulesElement name="Quick Blade" type="Feat" internal-id="ID_RYV_FEAT_41001" source="Ninja and Ki Power" revision-date="9/21/2010 11:07:50 AM">
    <Prereqs> Ninja, Stealthy Strike power </Prereqs>
    <print-prereqs> Ninja, Stealthy Strike power </print-prereqs>
    <specific name="Tier"> Heroic </specific>
    <specific name="Short Description"> Increase Stealthy Strike damage dice from d6 to d8 </specific>
    <rules>
      <modify name="Stealthy Strike" field="Special" value="Increase Stealthy Strike damage dice from d6 to d8."/>
    </rules>
