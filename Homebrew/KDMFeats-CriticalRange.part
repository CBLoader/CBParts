  <RulesElement name="Axe Mastery" type="Feat" internal-id="ID_FMP_FEAT_184" source="Player's Handbook" revision-date="1/26/2011 12:17:55 PM">
    <Prereqs> 21st level, Str 21, Con 15; Epic Tier </Prereqs>
    <print-prereqs> 21st level, Str 21, Con 15 </print-prereqs>
    <specific name="Tier"> Epic </specific>
    <specific name="Short Description"> Critical hit with axe melee attack roll of 19-20 </specific>
    <specific name="Special"/>
    <specific name="type"/>
    <specific name="Associated Power Info"/>
    <specific name="Associated Powers"/>
