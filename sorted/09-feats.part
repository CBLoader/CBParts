		<specific name="type"/>
		<specific name="Associated Power Info"/>
		<specific name="Associated Powers"/>
		<rules>
			<grant name="ID_FMP_LANGUAGE_18" type="Language"/>
			<statadd name="resist:necrotic" value="+2" type="resist"/>
