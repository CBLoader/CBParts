<!-- Skulker of Vhaeraun Theme -->
<RulesElement name="Skulker of Vhaeraun" type="Theme" internal-id="ID_TIV_THEME_DRAGON_MAGAZINE_413-9" source="Dragon Magazine 413">
<Prereqs> Drow </Prereqs>
<print-prereqs> Drow </print-prereqs>
    <specific name="_DisplayPowers"> ID_TIV_POWER_DRAGON_413-9-1,ID_TIV_POWER_DRAGON_413-9-2,ID_TIV_POWER_DRAGON_413-9-3 </specific>
    <specific name="_PARSED_SUB_FEATURES"> ID_TIV_CLASS_FEATURE_DRAGON_413-9-1,ID_TIV_CLASS_FEATURE_DRAGON_413-9-2,ID_TIV_CLASS_FEATURE_DRAGON_413-9-3 </specific>
