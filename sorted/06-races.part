	<RulesElement name="Ventriloquist Prank" type="Power" internal-id="ID_FMP_POWER_15837" source="Heroes of the Feywild" revision-date="00/00/0000 00:00:00 AM" >
		<Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_FMP_CLASS_0,ID_FMP_CATEGORY_10,ID_INTERNAL_CATEGORY_MINOR_ACTION,2 </Category>
