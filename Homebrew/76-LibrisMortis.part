</RulesElement>
<RulesElement name="Alternate Form" type="Power" internal-id="ID_DCY1_POWER_MV02" source="Libris Mortis, KDM Update" revision-date="01/30/2011" >
  <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_AT-WILL_USAGE,ID_DCY1_EPIC_DESTINY_MV01,ID_FMP_CATEGORY_15,ID_INTERNAL_CATEGORY_MINOR_ACTION,26 </Category>
  <Prereqs> ID_DCY1_EPIC_DESTINY_MV01 </Prereqs>
