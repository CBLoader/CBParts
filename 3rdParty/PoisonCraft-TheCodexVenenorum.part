	<RulesElement name="Flamestrike" type="Ritual" internal-id="ID_FMP_RITUAL_10010_30" source="Poisoncraft: The Codex Venenorum" revision-date="12/1/2010 5:45:29 PM">
		<specific name="Category"> Poison </specific>
