    <specific name="Powers"> ID_TIV_POWER_DRAGONMAGAZINE413_9,ID_TIV_POWER_DRAGONMAGAZINE413_11,ID_TIV_POWER_DRAGONMAGAZINE413_10 </specific>
    <specific name="_DisplayPowers"> ID_TIV_POWER_DRAGONMAGAZINE413_9,ID_TIV_POWER_DRAGONMAGAZINE413_11,ID_TIV_POWER_DRAGONMAGAZINE413_10 </specific>
    <rules>
