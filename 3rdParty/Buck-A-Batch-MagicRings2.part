  <RulesElement name="Blade Bane Ring (heroic tier)" type="Magic Item" internal-id="ID_EOF_MAGIC_ITEM_11001_07" source="Buck-A-Batch Magic Rings 2" revision-date="6/25/2011 2:06 AM">
