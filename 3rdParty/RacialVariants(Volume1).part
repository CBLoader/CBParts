  <RulesElement name="Unarmed Weapon Proficiency" type="Racial Trait" internal-id="ID_TIV_RACIAL_TRAIT_10" source="Racial Variants Volume 1" revision-date="12/24/2010 12:39:04 PM">
    <specific name="Short Description"> +3 bonus to unarmed attacks. Unarmed Damage 1d8. </specific>
    <specific name="Level"> 1 </specific>
