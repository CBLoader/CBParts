  <RulesElement name="Scalding Shield (heroic tier)" type="Magic Item" internal-id="ID_EOF_MAGIC_ITEM_11002_15" source="Buck-A-Batch Magic Shields 2" revision-date="6/25/2011 2:34 AM">
