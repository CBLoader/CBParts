  <RulesElement name="Dark Plague" type="Power" internal-id="ID_TIV_POWER_LIBRA_NECROMATICA_100_04" source="Libri Necromatica" revision-date="11/27/2010 11:30:01 AM">
    <Category> ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_ENCOUNTER,ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_TIV_CLASS_FH-SS_100_2,ID_TIV_CATEGORY_DEATH,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_12,ID_FMP_CATEGORY_25,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_14,ID_INTERNAL_CATEGORY_STANDARD_ACTION,1 </Category>
