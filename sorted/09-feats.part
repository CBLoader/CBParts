  <RulesElement internal-id="ID_FMP_FEAT_3743" name="War Wizard's Expertise" type="Feat" source="Dragon Magazine 402">
    <Prereqs> Any arcane class </Prereqs>
    <print-prereqs> Any arcane class </print-prereqs>
