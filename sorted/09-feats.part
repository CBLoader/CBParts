		<specific name="Short Description"> 1/encounter, make a saving throw at start of your turn </specific>
		<specific name="Special"/>
		<specific name="type"> Multiclass Warden </specific>
		<specific name="Associated Power Info"/>
		<specific name="Associated Powers"/>
		<rules>
			<grant name="ID_INTERNAL_COUNTSASCLASS_WARDEN" type="CountsAsClass"/>
			<grant name="ID_INTERNAL_MULTICLASS_MULTICLASS" type="Multiclass"/>
