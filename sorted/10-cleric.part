		<specific name="_DisplayPowers"> ID_FMP_POWER_14246,ID_FMP_POWER_14247,ID_FMP_POWER_14248,ID_FMP_POWER_14249 </specific>
		<rules>
			<grant name="ID_FMP_POWER_14246" type="Power" requires="ID_FMP_CLASS_705" />
