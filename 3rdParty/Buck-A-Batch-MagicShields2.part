  <RulesElement name="Basilisk Shield (paragon tier)" type="Magic Item" internal-id="ID_EOF_MAGIC_ITEM_11002_04" source="Buck-A-Batch Magic Shields 2" revision-date="6/25/2011 1:10 AM">
