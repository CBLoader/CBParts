  <RulesElement name="Next to Die" type="Power" internal-id="ID_FMP_POWER_15963" source="The Book of Vile Darkness">
    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_PARAGON_PATH_929,ID_FMP_CATEGORY_36,ID_INTERNAL_CATEGORY_MINOR_ACTION,12 </Category>
