  <RulesElement name="Master of Flowers" type="Paragon Path" internal-id="ID_CDJ_PARAGON_PATH_2150003" source="Advanced Player's Guide" revision-date="8/27/2010 12:40:19 PM">
    <Prereqs> Monk </Prereqs>
    <print-prereqs> Monk </print-prereqs>
    <Flavor> "Safety in numbers? It's an illusion. Watch." </Flavor>
    <specific name="Power Name"/>
    <specific name="Class Features"> ID_CDJ_CLASS_FEATURE_2150032, ID_CDJ_CLASS_FEATURE_2150033, ID_CDJ_CLASS_FEATURE_2150034 </specific>
    <specific name="Powers"> ID_CDJ_POWER_2150145,ID_CDJ_POWER_2150146,ID_CDJ_POWER_2150147 </specific>
