	<RulesElement name="Servant of Vice" type="Power" internal-id="ID_FMP_POWER_13819" source="Player Essentials: Heroes of Shadow" revision-date="3/10/2011 12:47:25 PM">
		<Category> ID_INTERNAL_CATEGORY_CLASS_POWER,ID_INTERNAL_CATEGORY_UTILITY,ID_FMP_CLASS_FEATURE_3571,ID_INTERNAL_CATEGORY_SWAPPABLE_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_CLASS_4,ID_FMP_CATEGORY_26,ID_FMP_CATEGORY_38,ID_INTERNAL_CATEGORY_MINOR_ACTION,4 </Category>
