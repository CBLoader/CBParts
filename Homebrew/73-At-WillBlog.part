  <RulesElement name="Surge of Speed" type="Power" internal-id="ID_RYV_POWER_100021" source="At-Will Blog" revision-date="10/12/2010 1:24:18 PM">
    <Category> ID_INTERNAL_CATEGORY_ENCOUNTER_USAGE,ID_INTERNAL_CATEGORY_LIMIT_BREAK,ID_FMP_CATEGORY_36,ID_INTERNAL_CATEGORY_MINOR_ACTION </Category>
