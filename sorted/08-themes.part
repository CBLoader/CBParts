  <RulesElement name="Minor Polymorph" type="Power" internal-id="ID_FMP_POWER_14130" source="Dragon Magazine 399" >
    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_CLASS_0,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_15,ID_INTERNAL_CATEGORY_STANDARD_ACTION,10,ID_FMP_CLASS_ORDER_ADEPT </Category>
