<RulesElement name="Apocalyptic Tremors" type="Power" internal-id="ID_FMP_POWER_TIV_10000_01" source="Ancient Wizard Magic" revision-date="11/21/2010 11:38:30 AM">
    <Category> ID_INTERNAL_CATEGORY_ATTACK,ID_INTERNAL_CATEGORY_AT-WILL,ID_INTERNAL_CATEGORY_AT-WILL_USAGE,ID_FMP_CLASS_9,ID_FMP_CATEGORY_31,ID_FMP_CATEGORY_20,ID_FMP_CATEGORY_25,ID_INTERNAL_CATEGORY_STANDARD_ACTION,1 </Category>
