  <RulesElement name="Storm Stomp Boots (paragon tier)" type="Magic Item" internal-id="ID_FMP_MAGIC_ITEM_TIV_10030_22_01" source="Buck-A-Batch Magic Boots (Book 1)" revision-date="12/10/2010 10:34:01 AM">
