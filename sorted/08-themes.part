</RulesElement>
<RulesElement name="Sea Chantey" type="Power" internal-id="ID_TIV_POWER_DRAGON_412-1" source="Dragon Magazine 412">
    <Category> ID_INTERNAL_CATEGORY_UTILITY,ID_INTERNAL_CATEGORY_DAILY_USAGE,ID_FMP_CATEGORY_36,ID_INTERNAL_CATEGORY_MINOR_ACTION,2,ID_FMP_CLASS_0 </Category>
