  <RulesElement name="Safeguard Shield" type="Class Feature" internal-id="ID_CDJ_CLASS_FEATURE_6440001" source="Quintessential Wizard" revision-date="12/03/10 11:41 PM">
    <Category> ID_FMP_CLASS_FEATURE_444,1 </Category>
    <Prereqs> !School of Magic Apprentice feat </Prereqs>
    <specific name="Short Description"> Encounter, immediate interrupt; with shield, force foe to reroll attack against you. </specific>
    <specific name="Level"> 1 </specific>
    <specific name="Powers"> ID_CDJ_POWER_6440074 </specific>
