  <RulesElement name="Frostbane Armor +1" type="Magic Item" internal-id="ID_FMP_MAGIC_ITEM_TIV_10025_35_01" source="Buck-A-Batch: Magic Armor (Book 2)" revision-date="12/8/2010 10:29:51 AM">
    <Flavor> This armor turns the tables on those who seek to harm you with cold. </Flavor>
    <specific name="Level"> 3 </specific>
    <specific name="Gold"> 680 </specific>
    <specific name="Magic Item Type"> Armor </specific>
    <specific name="Tier"/>
    <specific name="_Item_Set_ID"/>
